{
  "routes": [
    {
      "route": "/_profile",
      "allowedRoles": [
        "authenticated",
        "anonymous"
      ]
    }
  ],
  "auth": {
    "identityProviders": {
      "github": {
        "registration": {
          "clientIdSettingName": "GITHUB_CLIENT_ID",
          "clientSecretSettingName": "GITHUB_CLIENT_SECRET"
        }
      }
    }
  }
}